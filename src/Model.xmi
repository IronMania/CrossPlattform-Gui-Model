<?xml version="1.0" encoding="ASCII"?>
<gui:Application
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gui="http://www.example.org/gui"
    xsi:schemaLocation="http://www.example.org/gui metamodel/Gui.ecore"
    name="helloWorld"
    title="Hello World"
    namespace="de.example">
  <android targetsdk="12"
      minsdk="10"/>
  <content name="HalloWorldPage">
    <layout
        id="mainManager"
        type="vertical">
      <elements
          xsi:type="gui:LayoutManager">
        <elements
            xsi:type="gui:InputField"/>
        <elements
            xsi:type="gui:Button"/>
      </elements>
      <elements
          xsi:type="gui:Label"/>
    </layout>
  </content>
  <controller>
    <tabpages
        title=""
        tabcontent="//@content.0"/>
  </controller>
</gui:Application>
